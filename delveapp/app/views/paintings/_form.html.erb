<%= form_for(@painting) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :movement %>
  <%= f.text_field :movement %>

  <br>
    <ul>
    <%= f.collection_radio_buttons :artist, Artist.all, :id, :name %>
  </ul>
<br>

 <br>
 <ul>
   <%= f.collection_radio_buttons :gallery, Gallery.all, :id, :name %>
 </ul>
  <br>

  <%= f.submit %>
<% end %>
